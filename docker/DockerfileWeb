FROM docker-registry.intr/test-images/web:latest
COPY ./"${sdistName}" "${sdistName}"
RUN bash -c "virtualenv -p `which python3` te && \
				source te/bin/activate && \
				pip install --find-links file://`pwd`/${sdistName} taskexecutor"
ENV LANG=en_US.UTF-8
CMD ln -sf /bin/true /usr/sbin/setquota; /usr/local/te/bin/taskexecutor

